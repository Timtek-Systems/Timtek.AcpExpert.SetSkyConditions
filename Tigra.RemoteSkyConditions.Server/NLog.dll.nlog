    <target xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="Trace" name="DebugView"/>
  </targets>
<rules>
  <logger maxlevel="Debug" name="*" writeTo="DebugView" enabled="true" />
</rules>
